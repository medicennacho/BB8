#include "aplicacion.h"
#include "FW_rgb.h"

void rgb_init (void)
{
	//elijo que funcion van a cumplir los pines 0=gpio
	SetPINSEL(LEDR,0);
	SetPINSEL(LEDV,0);
	SetPINSEL(LEDA,0);

	//elijo salida o entrada 0 salida
	SetDIR(LEDR,0);
	SetDIR(LEDV,0);
	SetDIR(LEDA,0);

}
void rgb (void)
{
	SetPIN(LEDA,0);
	SetPIN(LEDV,1);

}
